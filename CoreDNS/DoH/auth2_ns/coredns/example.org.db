;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	d1.example.org. admin.example.org. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	 IN  NS d1.example.org.
d1.example.org. IN A 10.0.2.3

; name servers - NS records
example.org.     IN      NS      d1.example.org.
ns1.example.org. IN A 10.2.2.7
ns2.example.org. IN A 10.2.2.8
